@font-face {
    font-family: 'expletus_sansmedium';
    src: url('expletussans-medium-webfont.eot');
    src: url('expletussans-medium-webfont.eot?#iefix') format('embedded-opentype'),
         url('expletussans-medium-webfont.woff2') format('woff2'),
         url('expletussans-medium-webfont.woff') format('woff'),
         url('expletussans-medium-webfont.ttf') format('truetype'),
         url('expletussans-medium-webfont.svg#expletus_sansmedium') format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'expletus_sansregular';
    src: url('expletussans-regular-webfont.eot');
    src: url('expletussans-regular-webfont.eot?#iefix') format('embedded-opentype'),
         url('expletussans-regular-webfont.woff2') format('woff2'),
         url('expletussans-regular-webfont.woff') format('woff'),
         url('expletussans-regular-webfont.ttf') format('truetype'),
         url('expletussans-regular-webfont.svg#expletus_sansregular') format('svg');
    font-weight: normal;
    font-style: normal;
}

